              "operator": "equals",
              "loinc_code": "",
              "snomed_code": "2776000",
              "threshold_text": "present"
            }
          ]
        },
        "qualifiers": {
          "severity": [],
          "temporal": "",
          "persistence": "persisting after initial assessment"
        },
        "dependencies": []
