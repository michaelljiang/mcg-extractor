  "admission_decision_logic": {
    "rule_type": "disjunctive",
    "description": "Patient meets admission criteria if ANY of the following conditions are met",
    "minimum_criteria_count": 1,
    "criteria": [
      {
        "criterion_id": "criterion_001",
        "criterion_text": "Hemodynamic instability with systolic blood pressure less than 90 mmHg",
        "priority": "high",
        "clinical_category": "hemodynamic",
        "primary_condition": {
          "term": "Hemodynamic Instability",
          "snomed_code": "234171009",
          "icd10_codes": ["R57.9", "R65.21"],
          "synonyms": ["Circulatory instability", "Shock", "Cardiovascular collapse"]
        },
        "matching_conditions": {
          "logic_operator": "OR",
          "description": "Match if any of the following clinical findings are present",
          "conditions": [
            {
              "data_type": "vital_sign",
              "parameter": "Systolic Blood Pressure",
              "value": 90,
              "unit": "mmHg",
              "operator": "less_than",
              "loinc_code": "8480-6",
              "snomed_code": "271649006",
              "threshold_text": "less than 90 mmHg"
            },
            {
              "data_type": "vital_sign",
              "parameter": "Mean Arterial Pressure",
              "value": 65,
              "unit": "mmHg",
              "operator": "less_than",
              "loinc_code": "8478-0",
              "snomed_code": "6797001",
              "threshold_text": "less than 65 mmHg"
            }
          ]
        },
        "qualifiers": {
          "severity": ["severe"],
          "temporal": "",
          "persistence": ""
        },
        "dependencies": []
      },
      {
        "criterion_id": "criterion_002",
        "criterion_text": "Bacteremia, if blood cultures performed",
        "priority": "high",
        "clinical_category": "infectious",
        "primary_condition": {
          "term": "Bacteremia",
          "snomed_code": "5758002",
          "icd10_codes": ["A41.9"],
          "synonyms": ["Blood stream infection", "Septicemia"]
        },
        "matching_conditions": {
          "logic_operator": "AND",
          "description": "Match if blood cultures performed AND result is positive",
          "conditions": [
            {
              "data_type": "laboratory",
              "parameter": "Blood Culture",
              "value": "positive",
              "unit": "",
              "operator": "equals",
              "loinc_code": "600-7",
              "snomed_code": "61746007",
              "threshold_text": "positive result"
            }
          ]
        },
        "qualifiers": {
          "severity": [],
          "temporal": "",
          "persistence": ""
        },
        "dependencies": ["blood cultures performed"]
      },
      {
        "criterion_id": "criterion_003",
        "criterion_text": "Hypoxemia with oxygen saturation less than 90% despite supplemental oxygen",
        "priority": "high",
        "clinical_category": "respiratory",
        "primary_condition": {
          "term": "Hypoxemia",
          "snomed_code": "389086002",
          "icd10_codes": ["R09.02"],
          "synonyms": ["Low oxygen level", "Oxygen desaturation", "Hypoxia"]
        },
        "matching_conditions": {
          "logic_operator": "OR",
          "description": "Match if any of the following oxygen-related findings are present",
          "conditions": [
            {
              "data_type": "vital_sign",
              "parameter": "Oxygen Saturation",
              "value": 90,
              "unit": "%",
              "operator": "less_than",
              "loinc_code": "2708-6",
              "snomed_code": "431314004",
              "threshold_text": "less than 90%"
            },
            {
              "data_type": "laboratory",
              "parameter": "Partial Pressure of Oxygen (PaO2)",
              "value": 60,
              "unit": "mmHg",
              "operator": "less_than",
              "loinc_code": "2703-7",
              "snomed_code": "250775008",
              "threshold_text": "less than 60 mmHg"
            }
          ]
        },
        "qualifiers": {
          "severity": [],
          "temporal": "",
          "persistence": "despite supplemental oxygen"
        },
        "dependencies": ["supplemental oxygen administered"]
      },
      {
        "criterion_id": "criterion_004",
        "criterion_text": "Altered mental status persisting after initial assessment",
        "priority": "high",
        "clinical_category": "mental_status",
        "primary_condition": {
          "term": "Altered Mental Status",
          "snomed_code": "419284004",
          "icd10_codes": ["R41.82"],
          "synonyms": ["Confusion", "Delirium", "Decreased consciousness", "Altered level of consciousness"]
        },
        "matching_conditions": {
          "logic_operator": "OR",
          "description": "Match if any mental status alteration is present",
          "conditions": [
            {
