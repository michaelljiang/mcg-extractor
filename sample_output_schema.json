        "criterion_text": "Tachypnea that persists after observation period",
        "priority": "medium",
        "clinical_category": "respiratory",
        "primary_condition": {
          "term": "Tachypnea",
          "snomed_code": "271823003",
          "icd10_codes": ["R06.82"],
          "synonyms": ["Rapid breathing", "Increased respiratory rate"]
        },
        "matching_conditions": {
          "logic_operator": "SINGLE",
